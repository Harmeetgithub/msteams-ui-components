{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "interface-name": [
      false
    ],
    "member-access": [
      true,
      "no-public",
      "check-accessor",
      "check-constructor"
    ],
    "semicolon": [
      true,
      "always"
    ],
    "quotemark": [
      true,
      "single",
      "avoid-escape",
      "jsx-double"
    ],
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "never",
          "functions": "never",
          "typeLiterals": "ignore"
        }
      }
    ],
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator",
      "check-type",
      "check-typecast",
      "check-preblock"
    ],
    "no-empty-interface": false,
    "max-classes-per-file": [
      false
    ],
    "object-literal-sort-keys": [
      false
    ],
    "no-unused-variable": [
      true,
      {
        "ignore-pattern": "^_"
      }
    ],
    "no-var-requires": true,
    "callable-types": false,
    "forin": false
  },
  "rulesDirectory": []
}
